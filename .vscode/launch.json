  "version": "0.2.0",
  "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug App",
        "preLaunchTask": "tsc: build - tsconfig.json",
        "protocol": "inspector",
        "outFiles": [
          "${workspaceFolder}/lib/**/*.js"
        ],
        "env":{
          "NODE_ENV": "development",
          "PORT": "3001",
          "HEARTH_HOST": "localhost",
          "HEARTH_PORT": "3447"
        },
        "internalConsoleOptions": "neverOpen"
    }
  ]
}